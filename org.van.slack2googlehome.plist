<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>org.van.slack2googlehome</string>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/van/.nodebrew/current/bin/node</string>
        <string>/Users/van/Documents/scripts/slack2googlehome/server.js</string>
    </array>
    <key>WorkingDirectory</key>
    <string>/Users/van/Documents/scripts/slack2googlehome/</string>
    <key>StandardOutPath</key>
    <string>/var/log/vscript/slack2googlehome.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/vscript/slack2googlehome_err.log</string>
</dict>
</plist>

